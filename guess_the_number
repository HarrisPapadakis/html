<script>
rand=Math.floor(Math.random()*10)+1;
do {
x=prompt("Μάντεψε τον αριθμό");
if(x>rand) {
alert("Ο αριθμός που έδωσες είναι μεγαλύτερος");	
}
if(x<rand){
alert("Ο αριθμός που έδωσες είναι μικρότερος");	
}
}while(x!=rand);

alert("ΒΡΗΚΕΣ ΤΟΝ ΑΡΙΘΜΟ "+x+"!");

</script>
